{
  "name": "jobspeed",
  "version": "1.0.0",
  "description": "Firefox extension for auto-filling job applications",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "tsc && vite build && pnpm copy:icons",
    "copy:icons": "mkdir -p dist/icons && cp src/icons/*.svg dist/icons/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .ts,.tsx",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"{src,job-application-demo/src}/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"{src,job-application-demo/src}/**/*.{ts,tsx,json,css}\""
  },
  "keywords": [
    "firefox",
    "extension",
    "job-application",
    "autofill"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.18.1",
  "devDependencies": {
    "@types/firefox-webext-browser": "^143.0.0",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.1.3",
    "jsdom": "^27.0.1",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3",
    "vite": "^7.1.11",
    "vite-plugin-web-extension": "^4.1.6",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}

